{
  "demoApp": {
    "name": "ActionApp Demo",
    "version": "1.0.0-demo",
    "runtimeStrategy": [
      "auto",
      "node",
      "browser"
    ],
    "globals": {
      "apiPrefix": "/api/v1",
      "defaultLocale": "en-US",
      "supportedLocales": [
        "en-US",
        "es-MX"
      ],
      "featureFlags": {
        "demoMode": true,
        "loggingEnabled": true,
        "syncEnabled": true
      }
    },
    "entitySeeds": {
      "user_register": {
        "username": "demo_admin",
        "email": "demo@actionapp.local",
        "password": "demo!123",
        "role": "admin"
      },
      "action_space": {
        "name": "Demo Space",
        "description": "Sandbox for routing and entity testing",
        "config": {
          "type": "spa"
        }
      }
    },
    "routes": [
      {
        "method": "POST",
        "path": "/api/v1/users",
        "action": "user_register.create",
        "auth": "open"
      },
      {
        "method": "GET",
        "path": "/api/v1/users",
        "action": "user_register.read",
        "auth": "demo"
      },
      {
        "method": "GET",
        "path": "/api/v1/health",
        "action": "actionApp.health",
        "auth": "open"
      }
    ],
    "storageSamples": {
      "indexedDB": {
        "name": "demo_ehh",
        "version": 1,
        "stores": {
          "users": {
            "keyPath": "_id",
            "autoIncrement": true,
            "indexes": [
              { "name": "email", "keyPath": "email", "unique": true }
            ]
          }
        }
      },
      "localStorage": {
        "prefix": "demo_ehh",
        "ttl": 86400000
      },
      "filesystem": {
        "basePath": "shunya/config/data/demo",
        "format": "jsonl"
      }
    },
    "demoInstructions": [
      "Load demoApp.config into the actionApp loader before the runtime kicks off.",
      "Create an ActionApp instance with the runtimeStrategy values; capture the resolved runtime in demoApp.runtime.",
      "Iterate demoApp.globals to make variables available to templates and automation scripts.",
      "Use the `entitySeeds` section to prewarm user and workspace entities for quick CRUD interaction on the demo dashboard."
    ]
  }
}
