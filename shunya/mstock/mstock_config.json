{
    "name": "mStock",
    "baseURL": "https://api.mstock.trade",
    "wsURL": "wss://ws.mstock.trade",
    "endpoints": {
        "initiateLogin": "/openapi/typea/connect/login",
        "verifyOTP": "/openapi/typea/session/verifyotp",
        "placeOrder": "/openapi/typeb/orders",
        "modifyOrder": "/openapi/typeb/orders/{orderId}",
        "cancelOrder": "/openapi/typeb/orders/{orderId}",
        "placeBasketOrder": "/openapi/typea/basket",
        "orderBook": "/openapi/typea/orders",
        "holdings": "/openapi/typeb/portfolio/holdings",
